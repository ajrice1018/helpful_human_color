{
  "name": "helpful-human",
  "version": "1.0.0",
  "description": "Color swatch application",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run start:prod || yarn run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn run client\"",
    "client": "cd client && yarn run start",
    "install": "cd client && yarn install",
    "build": "cd client && yarn run build",
    "heroku-postbuild": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajrice1018/helpful_human_color.git"
  },
  "keywords": [
    "color",
    "swatch",
    "react",
    "node.js"
  ],
  "author": "Andrew Rice",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ajrice1018/helpful_human_color/issues"
  },
  "homepage": "https://github.com/ajrice1018/helpful_human_color#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.2",
    "config": "^3.2.2",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-validator": "^6.2.0",
    "graphql": "^14.5.4",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.17.1",
    "mysql2": "^1.7.0",
    "nodemon": "^1.19.2",
    "react-bootstrap": "^1.0.0-beta.12",
    "sequelize": "^5.18.1"
  }
}
